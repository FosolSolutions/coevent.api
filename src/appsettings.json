{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Urls": "https://*:443;http://*:80",
  "Serialization": {
    "Json": {
      "PropertyNameCaseInsensitive": true,
      "PropertyNamingPolicy": "CamelCase",
      "IgnoreNullValues": true
    }
  },
  "Authentication": {
    "Issuer": "Use Environment Variables",
    "Audience": "Use Environment Variables",
    "Secret": "Use Secret Variable!",
    "Salt": "Use Secret Variable!",
    "AccessTokenExpiresIn": "01:00:00"
  },
  "KeyVault": {
    "Name": "CoEvent",
    "Endpoint": "https://coevent.vault.azure.net",
    "ClientId": "adb9bc99-436c-467c-a680-5eb0521a83ff",
    "ClientSecret": "Use Secret Variable!"
  },
  "Swagger": {
    "RoutePrefix": "docs",
    "RouteTemplate": "/docs/swagger/{documentname}/swagger.json",
    "EndpointPath": "/docs/swagger/{0}/swagger.json"
  }
}
